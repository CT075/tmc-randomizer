CanSplit3;       Helper;; (|Items.SmithSword::4, Items.BlueSword, Items.FourSword), Helpers.HasSpin
CanSplit4;       Helper;; (|Items.SmithSword::5, Items.FourSword), Helpers.HasSpin

HasBottle;       Helper;; (|Items.Bottle1, Items.Bottle2, Items.Bottle3, Items.Bottle4)

HasBow;          Helper;; (|Items.Bow, Items.LightArrow)
