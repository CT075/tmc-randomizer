# Default logic for the randomizer - Doesn't allow glitches, but allows unintuitive tricks

!flag - KEYSANITY - Keysanity - false
!flag - SHUFFLE_ELEMENTS - Shuffle elements into item pool
!flag - NO_FUSIONS - No normal fusions - true
!flag - GLITCHLESS - Fix glitches - true
!flag - RANDOM_LANGUAGE - Randomize language
!flag - ALLOW_FUSION_SKIPS - Allow fusion skips - true
!flag - INSTANT_SKIP - Instant fusion skip
!flag - WHYWOULDYOUDOTHISTOYOURSELF - Obscure spots

#Settings

!ifdef - SHUFFLE_ELEMENTS
    globalElementsDefine;    Unshuffled; globalElements:Define:SecondByte;    Helpers.Inaccessible; Items.Untyped.01    # if defined, does not draw elements on map (during full shuffle)
!endif

!ifdef - KEYSANITY
    !define - KEYSANITY_MAJOR - Major
    !define - KEYSANITY_MINOR - Minor
	!define - DWS_SET - .18:
	!define - COF_SET - .19:
	!define - FOW_SET - .1A:
	!define - TOD_SET - .1B:
	!define - POW_SET - .1C:
	!define - DHC_SET - .1D:
	!define - RC_SET - .1E:
!else
    !define - KEYSANITY_MAJOR - DungeonItem
    !define - KEYSANITY_MINOR - DungeonItem
	!define - DWS_SET - :Deepwood
	!define - COF_SET - :FlameCave
	!define - FOW_SET - :Fortress
	!define - TOD_SET - :Droplets
	!define - POW_SET - :Palace
	!define - DHC_SET - :DHC
	!define - RC_SET - :Crypt
!endif

!ifdef - SHUFFLE_ELEMENTS
    !define - ELEMENT - Major
	!define - ELEMENT_DUNGEON - 
!else
    !define - ELEMENT - DungeonItem
	!define - ELEMENT_DUNGEON - Prizes
!endif

!ifdef - NO_FUSIONS
	noFusionDefine;							Unshuffled; noFusions:Define:SecondByte;					Helpers.Inaccessible; Items.Untyped.01	# if defined, fusions other than golden kinstone fusions are not available
!endif

!ifdef - GLITCHLESS
	glitchlessDefine;						Unshuffled; glitchless:Define:SecondByte;					Helpers.Inaccessible; Items.Untyped.01	# if defined, bugfixes are installed
!endif

!ifdef - RANDOM_LANGUAGE
	randomLanguageDefine;					Unshuffled; randomLanguage:Define:SecondByte;				Helpers.Inaccessible; Items.Untyped.01	# if defined, text changes language constantly
!endif

!ifdef - ALLOW_FUSION_SKIPS
	!ifdef - INSTANT_SKIP
		alwaysSkipFusionCutsceneDefine;		Unshuffled; alwaysSkipFusionCutscene:Define:SecondByte;		Helpers.Inaccessible; Items.Untyped.01	# if 0 fusion cutscenes are skipped when the buttons are held, if non-0 they are always instantly skipped
	!else
		alwaysSkipFusionCutsceneDefine;		Unshuffled; alwaysSkipFusionCutscene:Define:SecondByte;		Helpers.Inaccessible; Items.Untyped.00
	!endif
!else
	neverSkipFusionCutsceneDefine;			Unshuffled; neverSkipFusionCutscene:Define:SecondByte;		Helpers.Inaccessible; Items.Untyped.01	# if defined, fusion cutscenes are just like vanilla
!endif

#fuzzyDefine;						        Unshuffled; fuzzy:Define:SecondByte;                        Helpers.Inaccessible; Items.Untyped.14	# if defined with a value of 0 to E, makes the screen fuzzy and the pixels bigger
#followerDefine;                            Unshuffled; follower:Define:SecondByte;                     Helpers.Inaccessible; Items.Untyped.1	# if defined, a follower with the given id below will follow you throught he game
#followerIDDefine;                          Unshuffled; followerID:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1	# id for the above: 4=Mailman, 5=Zelda, 1E=Malon, 22=Smith, 24=King Dalton, 27=Dummy hero, 31=Cow, 32=Goron, 3C=Bow moblin, 45=Anju
#timedOhkoDefine;                           Unshuffled; timedohkoOption:Define:SecondByte;              Helpers.Inaccessible; Items.Untyped.1	# if defined, will enable ohko mode 30 minutes into the game
#timerDefine;                               Unshuffled; timerOption:Define:SecondByte;                  Helpers.Inaccessible; Items.Untyped.1	# if defined, will show a timer that counts up
#ohkoDefine;                                Unshuffled; ohkoOption:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1	# if defined link will die from any hit taken


#heartscolorDefine;                         Unshuffled; heartscolor:Define:SecondByte;                  Helpers.Inaccessible; Items.Untyped.1	# if defined will allow for special colored hearts
#heartFillRDefine;                          Unshuffled; heartFillR:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1F	# red   fill component (0-1F)
#heartFillGDefine;                          Unshuffled; heartFillG:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1F	# green fill component (0-1F)
#heartFillBDefine;                          Unshuffled; heartFillB:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1F	# blue  fill component (0-1F)
#heartEdgeRDefine;                          Unshuffled; heartEdgeR:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1F	# red   edge component (0-1F)
#heartEdgeGDefine;                          Unshuffled; heartEdgeG:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1F	# green edge component (0-1F)
#heartEdgeBDefine;                          Unshuffled; heartEdgeB:Define:SecondByte;                   Helpers.Inaccessible; Items.Untyped.1F	# blue  edge component (0-1F)


#barcolorDefine;                            Unshuffled; barcolor:Define:SecondByte;                     Helpers.Inaccessible; Items.Untyped.1	# if defined will allow for a different colored bar
#barcolorTypeDefine;                        Unshuffled; barcolorred:Define:SecondByte;                  Helpers.Inaccessible; Items.Untyped.1	# change the red in barcolorred to either red blue or green


# Location format:
#	Name;Type;Address[;Logic][;Item Override]
#		Name can be anything unique, hopefully without spaces
#		Type should be one of Location.LocationType
#		Address can be a hexadecimal address or of the form area-room-chest
#		Logic should be a comma separated series of conditions, which can take the forms:
#			(&a,b,c...) and (|a,b,c...) are an and/or of the contents
#			Items.(itemName)[.subValue] requires the given item for the location
#			Locations.(locationName) requires the location to be accessible first
#			Helpers.(helperName) is equivalent for Locations.(locationName), only provided for readability
#		Item Override should be in the form of Items.(itemName)[.subValue]

# Item Macro Helpers
